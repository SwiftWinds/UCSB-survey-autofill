{
  "name": "ucsb-covid-badge",
  "description": "A simple COVID badge replica website for UCSB dining commons",
  "version": "1.0.0",
  "homepage": "https://covidbadge.pages.dev",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@iconify/svelte": "^2.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@sveltejs/adapter-auto": "1.0.0-next.40",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "postcss": "^8.4.13",
    "postcss-load-config": "^3.1.4",
    "postcss-nesting": "^10.1.4",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "rollup": "^2.72.1",
    "svelte": "^3.48.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.4"
  },
  "type": "module",
  "dependencies": {
    "dayjs": "^1.11.2",
    "node-2fa": "^2.0.3",
    "svelte-themes": "^0.0.98"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SwiftWinds/ucsb-covid-badge.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SwiftWinds/ucsb-covid-badge/issues"
  }
}
